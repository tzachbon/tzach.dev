{
    "name": "ssr",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "prebuild": "yarn lint",
        "postbuild": "yarn moveBuildToRoot",
        "lint": "yarn run eslint",
        "lint:fix": "yarn run lint --fix",
        "start": "next start",
        "moveBuildToRoot": "yarn run copyfiles -u 1 \".next/**/*.*\" \"../../.next\" && yarn run copyfiles -u 1 \"out/**/*.*\" \"../../out\""
    },
    "dependencies": {
        "@react-three/cannon": "^1.1.1",
        "@react-three/drei": "^4.3.0",
        "@react-three/fiber": "^6.0.16",
        "next": "10.1.3",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-joystick-component": "^1.4.0",
        "three": "^0.128.0"
    },
    "devDependencies": {
        "@types/eslint": "^7.2.10",
        "@types/react": "^17.0.3",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "copyfiles": "^2.4.1",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-react": "^7.23.2",
        "prettier": "2.2.1",
        "typescript": "^4.2.4"
    }
}
